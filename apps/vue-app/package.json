    "eslint-plugin-oxlint": "~1.32.0",
    "eslint-plugin-vue": "~10.6.2",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.32.0",
    "prettier": "3.7.4",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@^7.2.11",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-tsc": "^3.1.8"
