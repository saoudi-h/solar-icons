    "@vueuse/core": "^13.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.560.0",
    "reka-ui": "^2.6.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "vue": "^3.5.25",
    "vue-color": "^3.3.3",
    "vue-router": "^4.6.4",
