{
    "name": "@solar-icons/vue",
    "version": "1.2.0-beta.1",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "description": "Solar Icons for Vue",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saoudi-h/solar-icons.git",
        "directory": "packages/vue"
    },
    "type": "module",
    "sideEffects": false,
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "LICENSE-THIRD-PARTY"
    ],
    "scripts": {
        "copy:licenses": "cp ../../LICENSE ./LICENSE && cp ../../LICENSE-THIRD-PARTY ./LICENSE-THIRD-PARTY",
        "generate:assets": "tsx scripts/generate-assets.ts",
        "build": "pnpm generate:assets && pnpm tsdown -l error && pnpm copy:licenses",
        "lint": "eslint . --max-warnings 0",
        "lint:fix": "eslint . --fix",
        "format:check": "prettier \"**/*\" --ignore-unknown --list-different",
        "format:fix": "prettier \"**/*\" --ignore-unknown --write",
        "clean": "rimraf *.tsbuildinfo node_modules build dist",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "typecheck": "tsc --noEmit",
        "pre-commit": "lint-staged"
    },
    "module": "./dist/index.mjs",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.cts",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs"
        },
        "./category": {
            "require": "./dist/category.cjs",
            "import": "./dist/category.mjs"
        },
        "./lib": {
            "require": "./dist/lib/index.cjs",
            "import": "./dist/lib/index.mjs"
        },
        "./package.json": {
            "default": "./package.json"
        }
    },
    "peerDependencies": {
        "vue": ">=3.0.0"
    },
    "devDependencies": {
        "@babel/core": "7.29.0",
        "@solar-icons/core": "workspace:*",
        "@vitejs/plugin-vue": "^6.0.4",
        "@vue/compiler-sfc": "3.5.27",
        "@vue/test-utils": "^2.4.6",
        "jsdom": "^27.4.0",
        "picocolors": "^1.1.1",
        "prettier-plugin-vue": "^1.1.6",
        "unplugin-vue": "^7.1.1",
        "vite": "^7.3.1",
        "vite-plugin-static-copy": "^3.2.0",
        "vitest": "^4.0.18",
        "vue": "^3.5.27",
        "vue-tsc": "3.2.4"
    },
    "license": "MIT"
}
