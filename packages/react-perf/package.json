{
    "name": "@solar-icons/react-perf",
    "version": "1.0.0",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "description": "Solar Icons for React with performance in mind",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saoudi-h/solar-icons.git",
        "directory": "packages/react-perf"
    },
    "type": "module",
    "sideEffects": false,
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "LICENSE-THIRD-PARTY"
    ],
    "scripts": {
        "build": "pnpm copy:licenses && tsdown",
        "copy:licenses": "cp ../../LICENSE ./LICENSE && cp ../../LICENSE-THIRD-PARTY ./LICENSE-THIRD-PARTY",
        "generate:assets": "tsx scripts/generate-assets.ts",
        "lint": "eslint . --max-warnings 0",
        "lint:fix": "eslint . --fix",
        "format:check": "prettier \"**/*\" --ignore-unknown --list-different",
        "format:fix": "prettier \"**/*\" --ignore-unknown --write",
        "clean": "rimraf *.tsbuildinfo node_modules build dist",
        "typecheck": "tsc --noEmit",
        "pre-commit": "lint-staged"
    },
    "module": "./dist/index.mjs",
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.mts",
            "import": "./dist/index.mjs"
        },
        "./lib": {
            "types": "./dist/lib/index.d.mts",
            "import": "./dist/lib/index.mjs"
        },
        "./Bold": {
            "types": "./dist/icons/style/Bold.d.mts",
            "import": "./dist/icons/style/Bold.mjs"
        },
        "./category/*/Bold": {
            "types": "./dist/icons/*/Bold.d.mts",
            "import": "./dist/icons/*/Bold.mjs"
        },
        "./BoldDuotone": {
            "types": "./dist/icons/style/BoldDuotone.d.mts",
            "import": "./dist/icons/style/BoldDuotone.mjs"
        },
        "./category/*/BoldDuotone": {
            "types": "./dist/icons/*/BoldDuotone.d.mts",
            "import": "./dist/icons/*/BoldDuotone.mjs"
        },
        "./Broken": {
            "types": "./dist/icons/style/Broken.d.mts",
            "import": "./dist/icons/style/Broken.mjs"
        },
        "./category/*/Broken": {
            "types": "./dist/icons/*/Broken.d.mts",
            "import": "./dist/icons/*/Broken.mjs"
        },
        "./LineDuotone": {
            "types": "./dist/icons/style/LineDuotone.d.mts",
            "import": "./dist/icons/style/LineDuotone.mjs"
        },
        "./category/*/LineDuotone": {
            "types": "./dist/icons/*/LineDuotone.d.mts",
            "import": "./dist/icons/*/LineDuotone.mjs"
        },
        "./Linear": {
            "types": "./dist/icons/style/Linear.d.mts",
            "import": "./dist/icons/style/Linear.mjs"
        },
        "./category/*/Linear": {
            "types": "./dist/icons/*/Linear.d.mts",
            "import": "./dist/icons/*/Linear.mjs"
        },
        "./Outline": {
            "types": "./dist/icons/style/Outline.d.mts",
            "import": "./dist/icons/style/Outline.mjs"
        },
        "./category/*/Outline": {
            "types": "./dist/icons/*/Outline.d.mts",
            "import": "./dist/icons/*/Outline.mjs"
        }
    },
    "devDependencies": {
        "@solar-icons/core": "workspace:*",
        "@solar-icons/eslint": "workspace:*",
        "@solar-icons/prettier": "workspace:*",
        "@solar-icons/tsconfig": "workspace:*",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.6",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.1",
        "dotenv": "^17.0.1",
        "picocolors": "^1.1.1",
        "prettier": "^3.6.2",
        "react": "19.2.0",
        "react-dom": "^19.2.0",
        "rimraf": "^6.1.2",
        "tsx": "^4.20.3",
        "typescript": "^5.9.3",
        "tsdown": "^0.16.6",
        "vitest": "^4.0.13"
    },
    "peerDependencies": {
        "react": ">= 16.8",
        "react-dom": ">= 16.8"
    },
    "engines": {
        "node": ">=16"
    },
    "keywords": [
        "react",
        "icons",
        "svg",
        "solar",
        "design",
        "interface",
        "phosphor",
        "UI",
        "UX"
    ],
    "author": {
        "name": "Saoudi Hakim",
        "url": "https://hakimsaoudi.dev",
        "email": "saoudihakim@gmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/saoudi-h/solar-icons/issues"
    }
}
