{
    "name": "@solar-icons/svelte",
    "version": "1.0.1",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "description": "Solar Icons for Svelte",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saoudi-h/solar-icons.git",
        "directory": "packages/svelte"
    },
    "type": "module",
    "sideEffects": false,
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "LICENSE-THIRD-PARTY"
    ],
    "scripts": {
        "build": "pnpm generate:assets && pnpm copy:licenses && pnpm build:package",
        "build:package": "svelte-package --input ./src",
        "copy:licenses": "cp ../../LICENSE ./LICENSE && cp ../../LICENSE-THIRD-PARTY ./LICENSE-THIRD-PARTY",
        "generate:assets": "tsx scripts/generate-assets.ts",
        "lint": "eslint . --max-warnings 0",
        "lint:fix": "eslint . --fix",
        "format:check": "prettier \"**/*\" --ignore-unknown --list-different",
        "format:fix": "prettier \"**/*\" --ignore-unknown --write",
        "clean": "rimraf *.tsbuildinfo node_modules build dist",
        "typecheck": "tsc --noEmit",
        "pre-commit": "lint-staged"
    },
    "module": "./dist/index.js",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "svelte": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./lib/*": {
            "types": "./dist/lib/*.d.ts",
            "svelte": "./dist/lib/*",
            "default": "./dist/lib/*.js"
        },
        "./category": {
            "types": "./dist/icons/index.d.ts",
            "svelte": "./dist/icons/index.js",
            "default": "./dist/icons/index.js"
        },
        "./category/*": {
            "types": "./dist/icons/*.d.ts",
            "svelte": "./dist/icons/*",
            "default": "./dist/icons/*.js"
        },
        "./*": {
            "types": "./dist/icons/style/*.d.ts",
            "svelte": "./dist/icons/style/*.js",
            "default": "./dist/icons/style/*.js"
        }
    },
    "devDependencies": {
        "@solar-icons/core": "workspace:*",
        "@solar-icons/eslint": "workspace:*",
        "@solar-icons/prettier": "workspace:*",
        "@solar-icons/tsconfig": "workspace:*",
        "@sveltejs/package": "^2.3.10",
        "@sveltejs/vite-plugin-svelte": "^5.0.3",
        "@types/node": "^25.0.3",
        "picocolors": "^1.1.1",
        "prettier": "^3.7.4",
        "prettier-plugin-svelte": "^3.4.1",
        "rimraf": "^6.1.2",
        "rollup-plugin-svelte": "^7.2.3",
        "svelte-check": "^4.1.4",
        "svelte-preprocess": "^6.0.3",
        "svelte2tsx": "^0.7.45",
        "tsdown": "^0.18.4",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "vitest": "^4.0.16"
    },
    "peerDependencies": {
        "svelte": ">= 4.0.0"
    },
    "engines": {
        "node": ">=16"
    },
    "keywords": [
        "svelte",
        "icons",
        "svg",
        "solar",
        "design",
        "interface",
        "phosphor",
        "UI",
        "UX"
    ],
    "author": {
        "name": "Saoudi Hakim",
        "url": "https://hakimsaoudi.dev",
        "email": "saoudihakim@gmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/saoudi-h/solar-icons/issues"
    }
}
